{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "env": {
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "no-console": 2,
    "no-return-await": 2,
    "no-duplicate-imports": 2,
    "no-unused-vars": 0
  }
}
