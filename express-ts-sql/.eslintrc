{
  "env": {
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-console": 2,
    "no-return-await": 2,
    "no-duplicate-imports": 2,
    "no-unused-vars": 0,
    "@typescript-eslint/no-unused-vars": ["error"]
  }
}
